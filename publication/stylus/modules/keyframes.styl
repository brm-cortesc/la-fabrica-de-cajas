@keyframes arbol
	0%
		transform rotate(0deg)
	25%
		transform rotate(10deg)
	50%
		transform rotate(0deg)
	75%
		transform rotate(-10deg)	
	100%
		transform rotate(0deg)
		
@keyframes sol
	0%
		transform rotate(0deg)
	25%
		transform rotate(30deg)
	50%
		transform rotate(0deg)
	75%
		transform rotate(-30deg)
	100%
		transform rotate(0deg)
		
@keyframes nubes
	0%
		transform translateX(0vh)
	25%
		transform translateX(-38vh)
	50%
		transform translateX(0em)
	75%
		transform translateX(38vh)
	100%
		transform translateX(0em)